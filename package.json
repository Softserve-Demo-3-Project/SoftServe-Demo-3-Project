{
  "name": "angular-webpack-seed",
  "version": "0.1.0",
  "description": "angualr project template",
  "main": "index.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "start": "npm-run-all --parallel server build-dev-run",
    "server": "json-server --watch db.json",
    "build": "webpack --config webpack-production.config.js --progress --colors",
    "build-dev": "webpack --config webpack.config.js --progress --colors",
    "build-dev-run": "webpack --config webpack.config.js --progress --colors | webpack-dev-server --config webpack-dev-server.config.js --progress --colors --port 2992 --inline --content-base public",
    "dev": "webpack-dev-server --config webpack-dev-server.config.js --progress --colors --port 2992 --inline --content-base public",
    "test": "karma start",
    "cover": "rimraf coverage && COVERAGE=1 karma start",
    "lint": "eslint ./src/"
  },
  "author": "Bogdan Savluk <savluk.bogdan@gmail.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "angular": "^1.6.4",
    "angular-animate": "^1.6.4",
    "angular-cookies": "^1.6.4",
    "angular-lodash": "^0.1.1",
    "angular-messages": "^1.6.4",
    "angular-route": "^1.6.4",
    "angular-sanitize": "^1.6.4",
    "angular-strap": "^2.3.12",
    "bootstrap": "^3.3.7"
  },
  "devDependencies": {
    "angular-mocks": "^1.6.4",
    "autoprefixer": "^7.1.0",
    "babel-core": "^6.24.1",
    "babel-istanbul-loader": "^0.1.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "codecov.io": "^0.1.6",
    "coveralls": "^2.13.1",
    "css-loader": "^0.28.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-loader": "^0.4.5",
    "jasmine-core": "^2.6.1",
    "json-server": "^0.10.1",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-cli": "^1.0.0",
    "karma-coverage": "^1.0.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^2.0.3",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "loader-utils": "^1.1.0",
    "ng-annotate-loader": "^0.6.0",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "postcss-loader": "^2.0.5",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  }
}
